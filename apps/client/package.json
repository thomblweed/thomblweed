{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm clean:build && pnpm build:css && pnpm build:remix",
    "build:remix": "remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "watch": "run-p \"watch:*\"",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "watch:remix": "cross-env NODE_ENV=development REMIX_ENV=development remix watch",
    "dev": "run-p \"dev:*\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "dev:remix": "cross-env NODE_ENV=development REMIX_ENV=development remix dev --port 2000",
    "start": "cross-env NODE_ENV=production netlify dev",
    "clean:build": "if [ -d \"public/build\" ]; then rm -rf \"public/build\"; fi",
    "lint": "eslint ."
  },
  "dependencies": {
    "@netlify/functions": "^1.4.0",
    "@planetscale/database": "^1.5.0",
    "@remix-run/netlify": "^1.11.1",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@supabase/auth-helpers-remix": "^0.1.4",
    "axios": "^1.2.6",
    "bcrypt": "^5.1.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
